{
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "blog-worker",
  "compatibility_date": "2025-11-15",
  "build": {
    "command": "npm run build"
  },
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page",
    "html_handling": "auto-trailing-slash"
  },
  "routes": [
    {
      "pattern": "blog.arusty.dev",
      "custom_domain": true
    }
  ]
}
